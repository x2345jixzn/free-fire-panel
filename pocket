// পারফরম্যান্সের জন্য DOM অপারেশন একসাথে করতে
const fastDOMOperation = () => {
  const target = document.getElementById('ddm_balance');
  if (!target) {
    console.error('Element with ID "ddm_balance" not found!');
    return;
  }
  
  // মাইক্রোটাস্ক ব্যবহার করে দ্রুত নির্বাহের জন্য
  setTimeout(() => {
    // এলিমেন্টের সমস্ত কন্টেন্ট রিমুভ (দ্রুততম পদ্ধতি)
    target.textContent = '';
    
    // নতুন কন্টেন্ট ইনজেক্ট
    target.insertAdjacentHTML('beforeend', customElement);
    console.log('✅ এলিমেন্ট ক্লিয়ার করে নতুন কন্টেন্ট যোগ করা হয়েছে!');
  }, 0);
};

fastDOMOperation();

// ব্যালেন্স সিঙ্ক্রোনাইজার ফাংশন (সংশোধিত ভার্সন)
const syncBalances = () => {
  // শুধুমাত্র প্রথম .js-balance-real-USD এলিমেন্ট সিলেক্ট করা
  const realBalance = document.querySelector('.balance-item--real .js-balance-real-USD');
  const demoBalance = document.querySelector('.js-balance-demo');
  
  if (!realBalance || !demoBalance) {
    console.error('Required elements not found!');
    return;
  }

  // ব্যালেন্স আপডেট ফাংশন
  const updateBalance = () => {
    const newBalance = demoBalance.textContent.trim();
    
    // সব প্রপার্টি আপডেট
    realBalance.textContent = newBalance;
    realBalance.dataset.hdShow = newBalance;
    realBalance.setAttribute('data-hd-show', newBalance);
    
    console.log(`ব্যালেন্স আপডেটেড: ${newBalance}`);
  };

  // MutationObserver + পোলিং কম্বাইন্ড সলিউশন
  let observer;
  let pollingInterval;
  let lastBalance = '';
  
  const startObserver = () => {
    try {
      observer = new MutationObserver(() => {
        const currentBalance = demoBalance.textContent.trim();
        if (currentBalance !== lastBalance) {
          lastBalance = currentBalance;
          updateBalance();
        }
      });
      
      observer.observe(demoBalance, {
        characterData: true,
        childList: true,
        subtree: true
      });
    } catch (e) {
      console.log('MutationObserver ব্যর্থ, পোলিং মোড চালু হচ্ছে');
      startPolling();
    }
  };
  
  const startPolling = () => {
    pollingInterval = setInterval(() => {
      const currentBalance = demoBalance.textContent.trim();
      if (currentBalance !== lastBalance) {
        lastBalance = currentBalance;
        updateBalance();
      }
    }, 300);
  };
  
  // প্রথম আপডেট (ইমিডিয়েট)
  updateBalance();
  
  // অবজার্ভার শুরু
  startObserver();
  
  // 3 সেকেন্ড পর পোলিং শুরু (যদি অবজার্ভার কাজ না করে)
  setTimeout(() => {
    if (!lastBalance) {
      startPolling();
    }
  }, 3000);
  
  // ক্লিনআপ ফাংশন (যদি পরবর্তীতে প্রয়োজন হয়)
  return () => {
    if (observer) observer.disconnect();
    if (pollingInterval) clearInterval(pollingInterval);
  };
};

// DOM রেডি হলে অথবা কনসোলে রান করলে立即执行
if (document.readyState === 'complete' || document.readyState === 'interactive') {
  syncBalances();
} else {
  document.addEventListener('DOMContentLoaded', syncBalances);
}

// কনসোলে立即执行的替代方案
setTimeout(syncBalances, 100);


// কাস্টম ব্যালেন্স সেটিং (এখানে মান পরিবর্তন করতে পারেন)
const FORMATTED_BALANCE = parseFloat(CUSTOM_BALANCE).toLocaleString('en-US', {
    minimumFractionDigits: 2,
    maximumFractionDigits: 2
});

// QT Demo এলিমেন্ট ট্র্যাকিং ফাংশন
function trackAndOverrideBalance() {
    const observer = new MutationObserver((mutations) => {
        mutations.forEach((mutation) => {
            if (mutation.type === 'childList' || mutation.type === 'characterData') {
                overrideBalance();
            }
        });
    });

    function overrideBalance() {
        const allDemoItems = document.querySelectorAll('a.balance-item[data-type="demo"]');
        let qtDemoBalance = null;

        allDemoItems.forEach(item => {
            const label = item.querySelector('.balance-item__label');
            if (label && label.textContent.trim() === 'QT Demo') {
                qtDemoBalance = item.querySelector('.js-balance-demo');
            }
        });

        if (qtDemoBalance) {
            // ব্যালেন্স ওভাররাইড
            if (qtDemoBalance.textContent !== FORMATTED_BALANCE || 
                qtDemoBalance.getAttribute('data-hd-show') !== FORMATTED_BALANCE) {
                
                qtDemoBalance.setAttribute('data-hd-show', FORMATTED_BALANCE);
                qtDemoBalance.textContent = FORMATTED_BALANCE;
                qtDemoBalance.setAttribute('data-hd-status', 'show');
                
                console.log(`ব্যালেন্স ওভাররাইড করা হয়েছে: $${FORMATTED_BALANCE}`);
            }
            
            // অবজার্ভার সেটআপ (যদি আগে থেকে না থাকে)
            if (!qtDemoBalance.__balanceObserverSet) {
                observer.observe(qtDemoBalance, {
                    characterData: true,
                    childList: true,
                    subtree: true
                });
                qtDemoBalance.__balanceObserverSet = true;
            }
        }
    }

    // প্রথমবার এক্সিকিউশন
    overrideBalance();
    
    // প্রতি 2 সেকেন্ডে চেক (ব্যাস্ট প্ল্যান)
    setInterval(overrideBalance, 2000);
}

// DOM রেডি হলে শুরু করুন
if (document.readyState === 'complete') {
    trackAndOverrideBalance();
} else {
    window.addEventListener('load', trackAndOverrideBalance);
}


/*********************** 
 * কনফিগারেশন সেকশন (আপনার কোড এখানে যোগ করুন) 
 ***********************/
const CONFIG = {
  enableModalRedirect: true, // false দিলে এই ফিচার অফ থাকবে
  modalId: "#demo-balance-modal" // কাস্টম মোডাল আইডি দিতে চাইলে এখানে দিন
};

/***********************
 * ইউটিলিটি ফাংশন সেকশন
 ***********************/
function waitForElement(selector, callback, maxAttempts = 10, interval = 500) {
  let attempts = 0;
  const checkInterval = setInterval(() => {
    const element = document.querySelector(selector);
    attempts++;
    
    if (element) {
      clearInterval(checkInterval);
      callback(element);
    } else if (attempts >= maxAttempts) {
      clearInterval(checkInterval);
      console.error(`Element ${selector} not found after ${maxAttempts} attempts`);
    }
  }, interval);
}

/***********************
 * মেইন ফাংশন সেকশন
 ***********************/
function setupModalRedirect() {
  if (!CONFIG.enableModalRedirect) return;
  
  waitForElement('a.btn.btn-green-v2.js-deposit-btn[data-deposit-source="header-balances-real"]', (topUpBtn) => {
    // মূল অ্যাট্রিবিউট সংরক্ষণ
    topUpBtn.dataset.originalHref = topUpBtn.href;
    topUpBtn.dataset.originalClass = topUpBtn.className;
    
    // মোডাল ভার্সনে পরিবর্তন
    topUpBtn.href = CONFIG.modalId;
    topUpBtn.classList.remove('js-deposit-btn');
    topUpBtn.classList.add('mfp-modal');
    topUpBtn.removeAttribute('data-deposit-source');
    
    // ইভেন্ট হ্যান্ডলার
    topUpBtn.addEventListener('click', handleTopUpClick);
    
    console.log('Modal redirect successfully setup');
  });
}

function handleTopUpClick(e) {
  e.preventDefault();
  
  // jQuery মোডাল যদি থাকে
  if (window.jQuery && jQuery.magnificPopup) {
    jQuery.magnificPopup.open({
      items: { src: CONFIG.modalId },
      type: 'inline'
    });
    return;
  }
  
  // ভ্যানিলা JS ফ্যালব্যাক
  const modal = document.querySelector(CONFIG.modalId);
  if (modal) {
    modal.style.display = 'block';
  } else {
    console.warn('Modal not found, falling back to original link');
    window.location.href = this.dataset.originalHref;
  }
}

/***********************
 * DOM পরিবর্তন ট্র্যাকিং সেকশন
 ***********************/
const observer = new MutationObserver((mutations) => {
  mutations.forEach((mutation) => {
    mutation.addedNodes.forEach((node) => {
      if (node.nodeType === 1) {
        const newBtn = node.querySelector?.('a.btn.btn-green-v2.js-deposit-btn[data-deposit-source="header-balances-real"]');
        if (newBtn && !newBtn.hasAttribute('data-redirect-configured')) {
          newBtn.href = CONFIG.modalId;
          newBtn.classList.remove('js-deposit-btn');
          newBtn.classList.add('mfp-modal');
          newBtn.removeAttribute('data-deposit-source');
          newBtn.setAttribute('data-redirect-configured', 'true');
          newBtn.addEventListener('click', handleTopUpClick);
        }
      }
    });
  });
});

/***********************
 * ইনিশিয়ালাইজেশন সেকশন
 ***********************/
function init() {
  // 1. প্রথমে আপনার কনফিগারেশন কোড এক্সিকিউট হবে
  console.log('কনফিগারেশন লোড সম্পন্ন');
  
  // 2. তারপর মোডাল রিডাইরেক্ট সেটআপ হবে
  if (CONFIG.enableModalRedirect) {
    setupModalRedirect();
    observer.observe(document.body, { childList: true, subtree: true });
  }
}

// DOM রেডি হলে শুরু করুন
if (document.readyState === 'complete') {
  init();
} else {
  window.addEventListener('load', init);
}
////   ////////////////////////////////////////////////////////
// Function to get saved stats from localStorage
function getSavedStats() {
    const savedStats = localStorage.getItem('realAccountStats');
    // If no saved stats, initialize with zero values
    return savedStats ? JSON.parse(savedStats) : { trades: 0, turnover: 0, profit: 0 };
}

// Function to save stats to localStorage
function saveStats(stats) {
    localStorage.setItem('realAccountStats', JSON.stringify(stats));
}

// Optimized trade tracking with efficient element observation
function initializeOptimizedTradeTracking() {
    // Configuration
    const config = {
        checkInterval: 500,
        maxRetries: 10
    };

    let retryCount = 0;
    let isInitialized = false;
    
    // Flag to track if the element was found and applied once
    let elementWasFound = false;
    let firstApplyDone = false; // Flag to check if the first apply has been done

    // Main initialization function
    function init() {
        const statsContainer = document.querySelector('.real-account-stats');
        const tradesContainer = document.querySelector('.deals-list');
        const refreshBtn = statsContainer?.querySelector('.js-refresh-content-icon');

        // Get saved stats
        let stats = getSavedStats();

        // Check if all required elements exist
        if (statsContainer && tradesContainer && refreshBtn) {
            console.log('All elements found - initializing trade tracking');
            
            // Calculate stats function
            function calculateStats() {
                const tradeItems = document.querySelectorAll('.deals-list__item');
                let newStats = { ...stats };

                // Count trades based on the number of items in the trade history
                newStats.trades = tradeItems.length; // We use the length of the tradeItems to count the trades
                newStats.turnover = 0;
                newStats.profit = 0;

                tradeItems.forEach(item => {
                    try {
                        const investmentText = item.querySelector('.item-row:nth-child(2) > div:first-child')?.textContent || '';
                        const profitText = item.querySelector('.item-row:nth-child(2) > div:last-child')?.textContent || '';
                        
                        const investmentMatch = investmentText.match(/\$([\d,]+\.?\d*)/);
                        const profitMatch = profitText.match(/-?\$([\d,]+\.?\d*)/);

                        if (investmentMatch) {
                            newStats.turnover += parseFloat(investmentMatch[1].replace(/,/g, ''));
                        }

                        if (profitMatch) {
                            const profit = parseFloat(profitMatch[1].replace(/,/g, ''));
                            const isProfit = item.querySelector('.price-up') || profitText.includes('+');
                            newStats.profit += isProfit ? profit : -profit;
                        }
                    } catch (e) {
                        console.error('Error processing trade item:', e);
                    }
                });

                // Save the updated stats to localStorage
                saveStats(newStats);
                return newStats;
            }

            // Update display function
            function updateDisplay() {
                const newStats = calculateStats();
                const tradesElement = statsContainer.querySelector('p:nth-child(3) span');
                const turnoverElement = statsContainer.querySelector('p:nth-child(4) span');
                const profitElement = statsContainer.querySelector('p:nth-child(5) span');

                if (tradesElement) tradesElement.textContent = newStats.trades;
                if (turnoverElement) turnoverElement.textContent = `$${newStats.turnover.toFixed(2)}`;
                if (profitElement) {
                    profitElement.textContent = `${newStats.profit >= 0 ? '$' : '-$'}${Math.abs(newStats.profit).toFixed(2)}`;
                    profitElement.className = newStats.profit >= 0 ? 'price-up' : 'price-down';
                }
            }

            // Setup mutation observer for trades
            const tradesObserver = new MutationObserver(() => {
                updateDisplay();
            });
            tradesObserver.observe(tradesContainer, { childList: true, subtree: true });

            // Handle the refresh button click event
            if (refreshBtn) {
                refreshBtn.addEventListener('click', () => {
                    // After refresh, simply re-display the stats (do not reset them)
                    updateDisplay();
                });
            }

            // Initial display
            updateDisplay();
            isInitialized = true;
            elementWasFound = true; // Element was found successfully

            console.log('Trade tracking initialized successfully');
        } else {
            // If elements are not found, only retry if they were previously found and then lost
            if (elementWasFound) {
                console.log('Elements lost, retrying...');
                retryCount = 0;  // Reset retry count to allow it to retry immediately
                elementWasFound = false;  // Mark element as lost
                setTimeout(init, config.checkInterval); // Retry after the specified interval
            } else {
                console.log('Max retries reached or elements not found after first loss.');
            }
        }
    }

    // Observe for the presence of the main element in the container
    const containerObserver = new MutationObserver(() => {
        const container = document.querySelector('.js-perfectScrollbar .ps');
        const mainElement = container?.querySelector('.real-account-stats');
        
        if (!mainElement && !firstApplyDone) {
            // If element is not found and first apply is not done, run init
            console.log('Main element lost, will reinitialize once it is not found');
            elementWasFound = false;
        } else if (!firstApplyDone) {
            // If the element is back and we haven't applied yet, apply once and clear localStorage
            console.log('Main element reappeared, initializing...');
            init(); // Initialize once the element is back and value is zero
            firstApplyDone = true; // Mark the first apply as done
            localStorage.clear(); // Clear localStorage after first apply
        }
    });

    // Start observing the container for changes in child nodes
    const container = document.querySelector('.js-perfectScrollbar .ps');
    if (container) {
        containerObserver.observe(container, {
            childList: true,
            subtree: true
        });
    }

    // Start initialization with debounce
    let initTimeout;
    function debouncedInit() {
        clearTimeout(initTimeout);
        initTimeout = setTimeout(init, 300);
    }

    // Observe DOM changes for dynamic content
    const domObserver = new MutationObserver((mutations) => {
        mutations.forEach((mutation) => {
            if (mutation.addedNodes.length) {
                debouncedInit();
            }
        });
    });

    domObserver.observe(document.body, {
        childList: true,
        subtree: true
    });

    // Initial check
    debouncedInit();
}

// Start the optimized tracking
initializeOptimizedTradeTracking();
// নতুন SVG টেমপ্লেট ফাংশন
function createNewSvg(size = 30) {
  const svg = document.createElementNS("http://www.w3.org/2000/svg", "svg");
  svg.setAttribute("viewBox", "0 0 30 30");
  svg.setAttribute("fill", "none");
  svg.setAttribute("xmlns", "http://www.w3.org/2000/svg");
  svg.setAttribute("width", size);
  svg.setAttribute("height", size);
  svg.innerHTML = `
